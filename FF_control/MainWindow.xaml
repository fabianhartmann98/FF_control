<Window x:Class="FF_control.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525"
        Background="{StaticResource BasicBackground}">
    <Window.Resources>
        <ResourceDictionary Source="Visual/Styles_dict.xaml"/>
    </Window.Resources>
    <StackPanel>
        <Label Content="asdf"/>
        <Button Content="sdfg"/>
        <ComboBox>
            <ComboBoxItem Content="asdf"/>
            <ComboBoxItem Content="qwer"/>
        </ComboBox>
        <TabControl>
            <TabItem Header="asdf" Style="{StaticResource Style_MainTabItem}">
                <Label Content="tabasdf"/>
            </TabItem>
            <TabItem Header="sdfg" Style="{StaticResource Style_MainTabItem}">
                <Label Content="tabsdfg"/>
            </TabItem>
        </TabControl>
    </StackPanel>
</Window>
